<launch> 
	<!-- Start test node-->

	<node pkg= "babs_wobbler_tests" type="lidar_calibration_test" name= "lidar_calibration_test" > 
	</node>

	<!-- Start lidar_calibration node-->

	<node pkg= "lidar_calibration"  type="lidar_calibration" name= "lidar_calibration" > 
	</node>

	<!-- Start rosbag or start devices? -->
	<node pkg="rosbag" type="play" name="player" output="screen" args="--clock ~/wobbler_scans/quickscan"/>

	<remap from="/sensor_msgs/LaserScan" to="/sick_scan" />
	<remap from="/wobbler_3d_cloud" to="/wobbler_sweep" />

</launch>
